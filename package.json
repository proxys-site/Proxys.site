{
  "name": "proxys-site",
  "version": "1.0.1",
  "description": "The official open-source codebase for Proxys.Site - A comprehensive proxy comparison tool and list.",
  "scripts": {
    "start": "npm run dev",
    "dev": "npx tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/styles.css --watch & eleventy --serve",
    "build:css": "npx tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/styles.css --minify",
    "build": "npm run build:css && rm -rf _site && eleventy"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "proxy",
    "proxy-list",
    "proxy-comparison",
    "proxys.site",
    "web-scraping",
    "residential-proxies",
    "datacenter-proxies"
  ],
  "author": "Proxys.Site Team <contact@proxys.site> (https://proxys.site)",
  "license": "MIT",
  "homepage": "https://proxys.site",
  "repository": {
    "type": "git",
    "url": "https://github.com/proxys-site/Proxys.site.git"
  },
  "bugs": {
    "url": "https://github.com/proxys-site/Proxys.site/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/proxys"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.23",
    "dotenv": "^16.3.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19"
  }

}
